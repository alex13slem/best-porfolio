{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "$components/*": ["src/components/*"],
      "$layouts/*": ["src/layouts/*"],
      "$assets/*": ["src/assets/*"],
      "$pages/*": ["src/pages/*"],
      "$styles/*": ["src/styles/*"],
      "$types/*": ["src/types/*"]
    }
  }
}
